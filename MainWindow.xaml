<Window x:Class="BandDatabaser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BandDatabaser"
        mc:Ignorable="d"
        Title="BandDatabaser" MinHeight="450" MinWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="220"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Rectangle Fill="#FFF4F4F5" Stroke="Black"/>
        <Button Content="Add band" Margin="10,50,10,0" MaxWidth="250" MinWidth="200" MinHeight="20" Click="Button_Click" Height="35" VerticalAlignment="Top"/>
        <Button Content="My library" Margin="10,10,10,0" VerticalAlignment="Top" MaxWidth="250" MinWidth="200" MinHeight="20" Height="35" Click="Button_Click"/>
        <Frame x:Name="MainFrame"  NavigationUIVisibility="Hidden" Grid.Column="1"/>
        <Button Content="Import from CSV" Margin="10,0,10,9.6" MaxWidth="250" MinWidth="200" MinHeight="20" Click="Button_Click" Height="35" VerticalAlignment="Bottom"/>
        <Button Content="Export to CSV" Margin="10,0,10,49.6" MaxWidth="250" MinWidth="200" MinHeight="20" Click="Button_Click" Height="35" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
